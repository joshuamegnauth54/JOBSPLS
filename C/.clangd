CompileFlags:
  Add:
    - '--std=c2x'
    - '-Wall'
    - '-Wextra'
    - '-Wformat=2'
    - '-Wconversion'
    - '-Wtrampolines'
    - '-Wimplicit-fallthrough'
    - '-U_FORTIFY_SOURCE'
    - '-D_FORTIFY_SOURCE=3'
    - '-D_GLIBCXX_ASSERTIONS'
    - '-fstrict-flex-arrays=3'
    - '-fstack-clash-protection'
    - '-fstack-protector-strong'
    - '-fcf-protection=full'
